/**index.wxss**/
.container {
  flex: 0 0 auto;
}
.margin-top-30 {
  margin-top: 30rpx;
}
.swiper {
  width: 100%;
  height: 350rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  box-shadow: 0 0 30rpx rgba(147, 166, 179, 0.8);
  border-radius: 12rpx;
  overflow: hidden;
}
.swiper .swiper-item {
  border-radius: 12rpx;
  overflow: hidden;
  box-sizing: border-box;
}
.swiper .slide-image {
  width: 100%;
}
.order {
  padding: 20rpx;
}
.order.empty {
  flex: 1;
}
.order.empty .div {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  height: 100%;
}
.leayer {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 1000;
  display: flex;
  align-items: center;
  justify-content: center;
}
.modal-tel {
  width: 80%;
  max-width: 700rpx;
  background-color: #fff;
  box-shadow: 0 0 30rpx rgba(147, 166, 179, 0.3);
  border-radius: 16rpx;
}
.modal-tel .modal-body {
  padding: 60rpx 40rpx;
}
.modal-tel .modal-body .label-input {
  display: flex;
  align-items: center;
  font-size: 32rpx;
  color: #4454cd;
}
.modal-tel .modal-body .label-input .iconfont {
  flex: 0 0 auto;
  margin-right: 10rpx;
}
.modal-tel .modal-body input,
.modal-tel .modal-body .input {
  height: 1.4rem;
  min-height: 1.4rem;
  line-height: 1.4rem;
  padding: 10rpx 0;
  font-size: 28rpx;
  border-bottom: 2rpx solid #efefef;
  border-radius: 8rpx;
}
.modal-tel .modal-body .inputFocus {
  border-bottom-color: #4454cd;
}
.modal-tel .modal-body .btn-cancel {
  width: 60%;
  margin: 60rpx auto 0;
  padding: 20rpx;
  text-align: center;
  background-color: #4454cd;
  color: #fff;
  font-size: 28rpx;
  font-weight: 500;
  border-radius: 40rpx;
}
.service-list {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  margin-left: -16rpx;
  margin-right: -16rpx;
  margin-top: 30rpx;
}
.service-list .item {
  flex: 0 0 33.33333%;
  padding: 16rpx;
  text-align: center;
  box-sizing: border-box;
  font-size: 28rpx;
}
.service-list .item-content {
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  top: 0;
  padding: 30rpx 20rpx;
  background-color: #fcfafa;
  border-radius: 16rpx;
  box-shadow: 0 0 20rpx rgba(147, 166, 179, 0.5);
  transition: all .1s;
  box-shadow: 0 8rpx 0 #cecece, 0 10rpx 4px rgba(0, 0, 0, 0.2);
}
.service-list .item-content.active {
  top: 8rpx;
  box-shadow: 0 8rpx 0 #bbbbbb, 0 10rpx 0 rgba(0, 0, 0, 0.2);
}
.service-list .item-content .text {
  position: relative;
  width: 120rpx;
  height: 120rpx;
  line-height: 120rpx;
  border-radius: 50%;
  font-size: 32rpx;
  border: 4rpx solid #f15a5a;
  background-color: #fff;
  color: #4454cd;
  font-weight: 500;
  z-index: 1;
}
.service-list .item:nth-child(9) .text {
  border-color: #f15a5a;
}
.service-list .item:nth-child(2) .text {
  border-color: #f19e5a;
}
.service-list .item:nth-child(4) .text {
  border-color: #85f15a;
}
.service-list .item:nth-child(7) .text {
  border-color: #5af1b2;
}
.service-list .item:nth-child(6) .text {
  border-color: #5ae7f1;
}
.service-list .item:nth-child(3) .text {
  border-color: #645af1;
}
.service-list .item:nth-child(5) .text {
  border-color: #ec5af1;
}
.service-list .item:nth-child(8) .text {
  border-color: #f15aa0;
}
