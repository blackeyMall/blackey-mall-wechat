page {
  display: flex;
  flex-direction: column;
  height: 100%;
  font-family: "PingFang SC";
}
.container {
  flex: 1;
  padding: 20rpx;
}
.title-page {
  display: flex;
  justify-content: center;
  flex-shrink: 0;
  padding: 30rpx;
  font-size: 50rpx;
  font-weight: 500;
  border-bottom: 2rpx solid #cbcbcb;
  background-color: #fff;
}
.title-page .avatar-img {
  width: 160rpx;
  height: 160rpx;
  margin: 0 0 0 auto;
  border-radius: 16rpx;
  background-color: #eff0fa;
  overflow: hidden;
  box-sizing: border-box;
}
.title-page .label {
  margin-top: 20rpx;
}
.title-channel {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  line-height: 1;
  margin: 60rpx 0 0;
  padding-bottom: 30rpx;
  border-bottom: 2rpx solid #ddd;
  font-weight: 700;
  font-size: 40rpx;
  color: #4454cd;
}
.title-channel .icon {
  width: 30rpx;
  height: 30rpx;
  margin: 0 20rpx;
  background: url(https://www.ssqushe.com/img/artisanImg/title-left.png) center center no-repeat;
  background-size: auto 100%;
}
.title-channel .icon-right {
  width: 30rpx;
  height: 30rpx;
  background: url(https://www.ssqushe.com/img/artisanImg/title-right.png) center center no-repeat;
  background-size: auto 100%;
}
.title-main {
  font-weight: 700;
  font-size: 40rpx;
  text-align: center;
}
.title-main .iconfont {
  margin-right: 10rpx;
  font-size: 44rpx;
}
.title-sm {
  font-size: 28rpx;
}
.title-sm > .div {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.title-sm .iconfont {
  font-size: 28rpx;
}
.label {
  display: inline-block;
  padding: 6rpx 10rpx;
  background-color: #f1f1f1;
  border-radius: 6rpx;
  text-align: center;
  font-size: 26rpx;
  box-sizing: border-box;
}
.label-info {
  background-color: #5f9ef7;
  color: #fff;
}
.label-success {
  background-color: #7ed67e;
  color: #fff;
}
.label-warning {
  background-color: #cab182;
  color: #fff;
}
.label-danger {
  background-color: #bd7676;
  color: #fff;
}
.label-primary {
  background-color: #6f7bda;
  color: #fff;
}
.label + .label {
  margin-top: 6rpx;
}
.text-primary {
  color: #4454cd;
}
.text-danger {
  color: #d34444;
}
.btn {
  width: 80%;
  margin: 0 auto 40rpx;
  padding: 20rpx;
  border-radius: 16rpx;
  background-color: #6f7bda;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
  box-sizing: border-box;
}
.contact {
  position: fixed;
  right: 20rpx;
  bottom: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100rpx;
  height: 100rpx;
  background-image: linear-gradient(to right bottom, #abb0d4, #6674e0);
  box-shadow: 0 0 16rpx rgba(147, 166, 179, 0.3);
  color: #fff;
  border-radius: 50%;
  transition: all .1s;
  opacity: .5;
  z-index: 10;
}
.contact:active {
  background-image: linear-gradient(to right bottom, #8d96d6, #3e51dd);
  opacity: 1;
}
.contact .iconfont {
  display: block;
  line-height: 1;
  font-size: 40rpx;
}
