<!--pages/reservationDetail/reservationDetail.wxml-->
<view class="title-page">
    <view class="div">
        <view>确认订单信息</view>
        <view class="label label-primary">{{orderInfo.statusname}}</view>
    </view>
    <image src="{{orderInfo.url}}" class="avatar-img" mode="aspectFit" />
</view>
<view class="form">
    <view class="form-group active">
        <label for="">预约姓名</label>
        <text>{{orderComfirmDetail.name}}</text>
    </view>
    <view class="form-group">
        <label class="" for="">预约电话</label>
        <text>{{orderComfirmDetail.telephone}}</text>
    </view>
    <view class="form-group">
        <label class="" for="">价格</label>
        <text>{{orderComfirmDetail.price}}</text>
    </view>
    <view class="form-group">
        <label class="" for="">预约日期</label>
        <text>{{orderComfirmDetail.serviceTime}}</text>
    </view>
    <view class="form-group">
        <label class="" for="">预约地址</label>
        <text>{{orderComfirmDetail.address}}</text>
    </view>
    <view class="form-group remarks">
        <label class="" for="">备注</label>
        <text>{{orderComfirmDetail.remark}}</text>
    </view>
</view>
<view class="btn" bindtap="onConfirmInfo">确定</view>
<view class="circle circle-pink"></view>
<view class="circle circle-purple"></view>
<view class="circle circle-blue"></view>