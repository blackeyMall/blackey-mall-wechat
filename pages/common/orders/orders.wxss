page {
  font-family: "PingFang SC";
}
.container {
  padding: 20rpx;
}
.title-page {
  padding: 0 30rpx 40rpx;
  font-size: 60rpx;
  font-weight: 500;
  border-bottom: 2rpx solid #cbcbcb;
  background-color: #fff;
}
.title-channel {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  width: 100%;
  line-height: 1;
  margin: 60rpx 0 40rpx;
}
.title-main {
  font-weight: 700;
  font-size: 40rpx;
}
.title-main > .div {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.title-main .iconfont {
  margin-right: 10rpx;
  font-size: 44rpx;
}
.title-sm {
  font-size: 28rpx;
}
.title-sm > .div {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.title-sm .iconfont {
  font-size: 28rpx;
}
.label {
  width: 80rpx;
  padding: 6rpx 10rpx;
  background-color: #f1f1f1;
  border-radius: 6rpx;
  text-align: center;
  font-size: 20rpx;
}
.label-info {
  background-color: #5f9ef7;
  color: #fff;
}
.label-success {
  background-color: #7ed67e;
  color: #fff;
}
.label-warning {
  background-color: #cab182;
  color: #fff;
}
.label-danger {
  background-color: #bd7676;
  color: #fff;
}
.label-primary {
  background-color: #6f7bda;
  color: #fff;
}
.label + .label {
  margin-top: 6rpx;
}
.text-primary {
  color: #4454cd;
}
.text-danger {
  color: #d34444;
}
.order-list .item {
  display: flex;
  flex-wrap: wrap;
  height: 100%;
  padding: 30rpx;
  background-color: #fff;
  border-radius: 16rpx;
  box-shadow: 0 0 16rpx rgba(147, 166, 179, 0.3);
  font-size: 24rpx;
}
.order-list .item .iconfont {
  font-size: 24rpx;
}
.order-list .item-image {
  position: relative;
  width: 146rpx;
  height: 146rpx;
  margin-right: 30rpx;
  border-radius: 16rpx;
  box-shadow: 0 0 8rpx rgba(147, 166, 179, 0.3);
  overflow: hidden;
}
.order-list .item-image .img {
  position: relative;
  top: 50%;
  width: 146rpx;
  height: 100%;
  transform: translateY(-50%);
}
.order-list .item-info {
  flex: 1;
}
.order-list .item-info > .div {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 100%;
}
.order-list .item-title {
  margin-bottom: 20rpx;
  color: #4454cd;
}
.order-list .item-date {
  margin-bottom: 10rpx;
  color: #999;
  font-size: 24rpx;
}
.order-list .item-address {
  color: #666;
  font-size: 24rpx;
}
.order-list .item-title > .div,
.order-list .item-date > .div,
.order-list .item-address > .div {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.order-list .item-title > .div .iconfont,
.order-list .item-date > .div .iconfont,
.order-list .item-address > .div .iconfont {
  margin-right: 10rpx;
}
.order-list .item-title > .div text,
.order-list .item-date > .div text,
.order-list .item-address > .div text {
  flex: 1;
  text-align: justify;
}
.order-list .item-price {
  text-align: right;
  font-weight: 500;
}
.order-list .item-operator {
  margin-left: 20rpx;
}
.order-list .item-operator .label {
  margin-left: auto;
  margin-top: 30rpx;
}
.order-list .item + .item {
  margin-top: 20rpx;
}
.order-list p {
  text-align: justify;
}
