page {
  display: flex;
  flex-direction: column;
  height: 100%;
  font-family: "PingFang SC";
}
.container {
  flex: 1;
  padding: 20rpx;
}
.title-page {
  display: flex;
  justify-content: space-between;
  flex-shrink: 0;
  padding: 30rpx;
  font-size: 50rpx;
  font-weight: 500;
  border-bottom: 2rpx solid #cbcbcb;
  background-color: #fff;
}
.title-page .avatar-img {
  width: 160rpx;
  height: 160rpx;
  margin: 0 0 0 auto;
  border-radius: 16rpx;
  background-color: #eff0fa;
  overflow: hidden;
  box-sizing: border-box;
}
.title-page .label {
  margin-top: 20rpx;
}
.title-channel {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  width: 100%;
  line-height: 1;
  margin: 60rpx 0 40rpx;
}
.title-main {
  font-weight: 700;
  font-size: 40rpx;
}
.title-main > .div {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.title-main .iconfont {
  margin-right: 10rpx;
  font-size: 44rpx;
}
.title-sm {
  font-size: 28rpx;
}
.title-sm > .div {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.title-sm .iconfont {
  font-size: 28rpx;
}
.label {
  width: 80rpx;
  padding: 6rpx 10rpx;
  background-color: #f1f1f1;
  border-radius: 6rpx;
  text-align: center;
  font-size: 26rpx;
}
.label-info {
  background-color: #5f9ef7;
  color: #fff;
}
.label-success {
  background-color: #7ed67e;
  color: #fff;
}
.label-warning {
  background-color: #cab182;
  color: #fff;
}
.label-danger {
  background-color: #bd7676;
  color: #fff;
}
.label-primary {
  background-color: #6f7bda;
  color: #fff;
}
.label + .label {
  margin-top: 6rpx;
}
.text-primary {
  color: #4454cd;
}
.text-danger {
  color: #d34444;
}
.btn {
  width: 80%;
  margin: 0 auto 40rpx;
  padding: 20rpx;
  border-radius: 16rpx;
  background-color: #6f7bda;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
  box-sizing: border-box;
}
