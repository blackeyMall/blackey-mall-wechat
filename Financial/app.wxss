}

.padding-bottom-108 {
    padding-bottom: 108rpx;
}

.padding-bottom-110 {
    padding-bottom: 110rpx;
}

.padding-bottom-20 {
    padding-bottom: 20rpx !important;
}
.no-padding-bottom {
    padding-bottom: 0 !important;
}

.margin-bottom-10 {
    margin-bottom: 10rpx !important;
}

.margin-bottom-20 {
    margin-bottom: 20rpx !important;
}
/* ---/utils */


page {
    height: 100%;
    background-color: #f3f3f3;
}

.avatar {
    border-radius: 50%;
}

.panel {
    padding: 30rpx;
    background-color: #fff;
    font-size: 28rpx;
}

.panel + .panel {
    margin-top: 20rpx;
}

.panel .nav-tab {
    padding: 0;
}

.nav-tab {
    position: relative;
    display: flex;
    justify-content: center;
    background-color: #fff;
    padding: 30rpx 30rpx 0;
}

.nav-tab .item {
    min-width: 120rpx;
    padding: 0 30rpx;
    text-align: center;
    font-size: 28rpx;
}

.nav-tab .item.active {
    color: #67a6f3;
}

.nav-tab .item .text {
    position: relative;
    display: inline-block;
    padding-bottom: 20rpx;
    transition: all .2s;
}

.nav-tab .item .text::after {
    position: absolute;
    bottom: 0;
    left: 50%;
    width: 0;
    height: 6rpx;
    border-radius: 6rpx;
    background-color: transparent;
    transition: all .3s;
    transform: translateX(-50%);
    content: ' ';
}

.nav-tab .item.active .text::after {
    width: 70rpx;
    background-color: #69a2ef;
}

.nav-tab-sub {
    display: flex;
    font-size: 26rpx;
}

.nav-tab-sub .item {
    flex: 1;
    padding: 20rpx;
    text-align: center;
    background-color: #fff;
    border-top: 2rpx solid #f3f3f3;
    border-bottom: 2rpx solid #f3f3f3;
}

.nav-tab-sub .item.active {
    color: #69a2ef;
}

.tag-list {
    display: flex;
    align-items: center;
    line-height: 1;
    color: #666;
    font-size: 22rpx;
}

.tag-list .tag-item + .tag-item {
    margin-left: 10rpx;
    padding-left: 10rpx;
    border-left: 2rpx solid #999;
}

.businessCard {
    position: relative;
}

.businessCard::before {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 70%;
    border-bottom-left-radius: 50%;
    border-bottom-right-radius: 50%;
    background: linear-gradient(to bottom, #67a6f3, #3968d1);
    content: ' ';
}

.businessCard .card {
    position: relative;
    margin-bottom: 30rpx;
    padding: 40rpx;
    border-radius: 8rpx;
    box-shadow: 0 0 40rpx rgba(0, 0, 0, .3);
    background-color: #fff;
    z-index: 1;
}

.businessCard .card .icon-open-right {
    position: absolute;
    top: 50%;
    right: 20rpx;
    transform: translateY(-50%);
    font-size: 24rpx;
}

.businessCard .card-header {
    position: relative;
    display: flex;
    align-items: center;
}

.businessCard .btn-share {
    position: absolute;
    top: 0;
    right: 0;
    display: flex;
    align-items: center;
    padding: 20rpx;
    line-height: 1;
    font-size: 24rpx;
    color: #e9a04c;
    background-color: transparent;
    z-index: 10;
}

.businessCard .btn-share .iconfont {
    margin-right: 10rpx;
    font-size: 28rpx;
}

.businessCard .btn-share::after {
    content: none;
}

.businessCard .card-header .avatar {
    flex: 0 0 140rpx;
    height: 140rpx;
}

.businessCard .card-header .info {
    flex: 1;
    padding: 0 0 0 30rpx;
    color: #373737;
    overflow: hidden;
}

.businessCard .card-header .name {
    font-size: 36rpx;
    font-weight: 500;
    color: #0d0d0d;
}

.businessCard .card-header .tag {
    margin-top: 16rpx;
}

.businessCard .card-header .item {
    display: block;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    text-align: justify;
}

.businessCard .card-body {
    margin-top: 30rpx;
    color: #67a6f3;
    font-size: 24rpx;
}

.businessCard .info-list .item {
    display: flex;
    line-height: 1.4;
}

.businessCard .info-list .item + .item {
    margin-top: 10rpx;
}

.businessCard .info-list .item .icon {
    width: 50rpx;
}

.businessCard .info-list .item .iconfont {
    line-height: 1;
    color: #67a6f3;
}

.businessCard .more {
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 24rpx;
    color: #67a6f3;
    z-index: 1;
}

.businessCard .more .iconfont {
    display: block;
    margin-left: 10rpx;
    font-weight: 500;
    font-size: 30rpx;
    transition: all .3s;
}

.businessCard .more.active .iconfont {
