/* ---utils */
.h-310 {
    height: 310rpx;
}
.relative {
    position: relative;
}
.text-right {
    text-align: right;
}
.fixed-bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 2;
}
.flex-column {
    display: flex;
    flex-direction: column;
    height: 100%;
}
.flex-main {
    flex: 1;
    -webkit-overflow-scrolling: touch;
    overflow-y: auto;
}
.flex-center {
    display: flex;
    align-items: center;
    width: 100%;
}
.justify-between {
    justify-content: space-between !important;
}
.input-placeholder {
    color: #aab1bc;
}
.label-item {
    display: flex;
    align-items: center;
    font-size: 20rpx;
    padding: 4rpx 10rpx;
    border: 2rpx solid #e9a04c;
    color: #e9a04c;
}

.label-item + .label-item {
    margin-left: 10rpx;
}

.label {
    padding: 6rpx 10rpx;
    font-size: 24rpx;
    border-radius: 6rpx;
}

.label-danger {
    border: 2rpx solid #f56c6c;
    color: #f56c6c;
}

.label-success {
    border: 2rpx solid #67c23a;
    color: #67c23a;
}

.btn-primary {
    padding: 20rpx;
    background-color: #67a6f3;
    color: #fff;
    text-align: center;
    font-size: 34rpx;
}

.text-warning {
    color: #e4b27a;
}

.text-danger {
    font-weight: 500;
    color: #f56c6c;
}

.letter-space-10 {
