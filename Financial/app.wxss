.toastr {
    position: fixed;
    top: 20rpx;
    right: 20rpx;
    display: flex;
    align-items: center;
    padding: 20rpx 30rpx;
    font-size: 24rpx;
    background-color: #fff;
    border-radius: 6rpx;
    box-shadow: 0 0 20rpx rgba(0, 0, 0, .2);
    animation: toastrHide .5s ease-in-out both;
    z-index: 1002;
}

.toastr.active {
    animation: toastrShow .5s ease-in-out both;
}

.toastr text {
    margin: 0 10rpx;
}

.toastr .iconfont {
    margin-right: 10rpx;
    font-size: 36rpx;
}

@keyframes toastrHide {
    0% {
        right: 20rpx;
        opacity: 1;
    }
    100% {
        right: -100%;
        opacity: 0;
    }
}

@keyframes toastrShow {
    0% {
        right: -100%;
        opacity: 0;
    }
    100% {
        right: 20rpx;
        opacity: 1;
    }
}

.leayer {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: rgba(0, 0, 0, .5);
    z-index: 1000;
}

.modal {
    width: 80%;
    background-color: #fff;
    box-shadow: 0 0 30rpx rgba(0, 0, 0, .4);
    border-radius: 8rpx;
}

.modal .modal-header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx 30rpx;
    border-bottom: 2rpx solid #f3f3f3;
}

.modal .modal-header .title {
    font-size: 32rpx;
    color: #000;
